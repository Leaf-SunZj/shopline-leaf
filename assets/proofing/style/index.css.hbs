  font-weight:var(--type-title-font-weight, 700);
  letter-spacing:var(--type-title-letter-spacing);
  line-height:var(--type-title-line-height);
}

.title1{
  margin-bottom:0;
  margin-top:0;
  word-break:normal;
}
[data-type_title_uppercase=true] .title1{
